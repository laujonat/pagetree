{
    "name": "pagetree-inspector-extension",
    "version": "1.0.0",
    "description": "Visualize distribution of DOM nodes in the document tree",
    "scripts": {
        "watch": "cross-env NODE_ENV=development webpack --config build/webpack.config.dev.ts --watch",
        "build": "run-p clean webpack:bundle",
        "webpack:bundle": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.ts",
        "stats": "cross-env NODE_ENV=production webpack --config build/webpack.analysis.ts",
        "clean": "rimraf dist",
        "test": "npx jest",
        "lint": "run-p lint:ts lint:es lint:style lint:prettier",
        "lint:ts": "tsc --noEmit --skipLibCheck",
        "lint:style": "stylelint 'src/styles/**/*.css'",
        "lint:prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
        "lint:es": "eslint --config ./.eslintrc.cjs **/*.{ts,tsx}",
        "precommit": "lint-staged",
        "prepare": "husky install"
    },
    "author": "Jonathan Lau",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/laujonat/pagetree.git"
    },
    "dependencies": {
        "@babel/runtime": "^7.23.5",
        "@types/d3": "^7.4.3",
        "clone": "^2.1.2",
        "d3": "^7.8.5",
        "prismjs": "^1.29.0",
        "react": "^18.2.0",
        "react-d3-tree": "^3.6.1",
        "react-dom": "^18.2.0",
        "react-svg-text": "^0.1.2",
        "use-events": "^1.4.2"
    },
    "chromeExtension": {
        "manifest-version": 3
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": "eslint --fix",
        "*.{ts,tsx,html,css,md}": "prettier --write"
    },
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.23.4",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@types/chrome": "^0.0.246",
        "@types/jest": "^29.5.0",
        "@types/react": "^18.2.42",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^9.1.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "eslint": "^8.53.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "glob": "^7.1.6",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jsdom": "^22.1.0",
        "lint-staged": "^15.2.0",
        "mini-css-extract-plugin": "^2.7.6",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2 ",
        "style-loader": "^3.3.3",
        "stylelint": "^15.11.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-order": "^6.0.3",
        "terser-webpack-plugin": "^5.3.9",
        "ts-jest": "^29.1.0",
        "ts-loader": "^8.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2",
        "webpack": "^5.76.0",
        "webpack-bundle-analyzer": "^4.10.1",
        "webpack-cli": "^4.0.0",
        "webpack-merge": "^5.0.0"
    },
    "volta": {
        "node": "18.19.0"
    }
}